type Series @key(fields: "id") {
  id: ID!
  title: String!
  parts(skip: Int, limit: Int): [SeriesPart!]!
}

extend type Query {
  series(id: ID!): Series!
  allSeries: [Series!]!
}

input CreateSeriesInput {
  title: String!
}

extend type Mutation {
  createSeries(data: CreateSeriesInput!): Series!
}
